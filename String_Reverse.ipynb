    "#Program to print reverse of a String\n",
    "def rev_str(input_str):\n",
    "    return input_str[ : : -1]\n",
    "\n",
    "#Getting Input from User\n",
    "string = input(\"Enter a String : \")\n",
    "\n",
    "#Calling the function to print the result\n",
    "result = rev_str(string)\n",
    "print(\"Reversed String is : \", result)"
   ]
  }
